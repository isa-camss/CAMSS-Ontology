    cav:value "1"^^xsd:int .

camssa:5bc9e8ce-30bf-4b5a-be51-d53e655d992d a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "XAdES Baseline Profile function is to facilitate the electronic signature in compliance with eIDAS, so it can be concluded that through it processes, data and services are fostered."@en ;
    cav:refersTo camssa:c61913b5-b7e3-4c28-872c-7851a18438b4 .

camssa:5c4c48c6-2276-4586-978c-d0cae3bbdb25 a cav:Score,
